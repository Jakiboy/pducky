{
	"name": "jakiboy/pducky",
	"version": "0.2.0",
	"type": "library",
	"description": "Run fast SQL queries on massive datasets (CSV, JSON, and Parquet) powered by DuckDB.",
	"keywords": [
		"DuckDB",
		"SQL",
		"SQLite3",
		"CSV",
		"JSON",
		"XML",
		"Parquet",
		"Data Analysis",
		"Big Data",
		"PHP"
	],
	"homepage": "https://github.com/Jakiboy/pducky",
	"license": "MIT",
	"authors": [
		{
			"name": "Jihad Sinnaour",
			"email": "me@jihadsinnaour.com",
			"role": "Founder"
		}
	],
	"require": {
		"php": ">=7.4",
		"ext-zip": "*"
	},
	"autoload": {
		"psr-4": {
			"Pducky\\": "src/"
		}
	},
	"bin": [
		"bin/pducky"
	],
	"scripts": {
		"install-binaries": "php bin/pducky install-binaries",
		"check-binaries": "php bin/pducky check",
		"post-install-cmd": [
			"php bin/pducky install-binaries"
		],
		"post-update-cmd": [
			"php bin/pducky install-binaries"
		]
	},
	"scripts-descriptions": {
		"install-binaries": "Install DuckDB binaries",
		"check-binaries": "Check DuckDB binary installation status"
	},
	"archive": {
		"exclude": [
			".github",
			".gitignore",
			"temp",
			"*.workspace"
		]
	}
}